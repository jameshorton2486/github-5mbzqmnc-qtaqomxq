{
  "name": "lovable-depo-pro",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "api": "node api/server.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run api\"",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md}\"",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:env": "tsx src/lib/test-env.ts",
    "test:deepgram": "tsx src/lib/test-deepgram.ts",
    "test:supabase": "tsx src/lib/test-supabase.ts",
    "validate:env": "tsx src/lib/validate-env.ts",
    "validate": "npm run validate:env && npm run test:supabase && npm run test:deepgram",
    "analyze:ts": "tsx scripts/analyzeTypeScript.ts",
    "fix:ts": "tsx scripts/analyzeTypeScript.ts --fix"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.7",
    "axios": "^1.6.7",
    "busboy": "^1.6.0",
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.5",
    "fluent-ffmpeg": "^2.1.2",
    "lucide-react": "^0.344.0",
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.1",
    "youtube-dl-exec": "^2.5.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@eslint/js": "^8.56.0",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "globals": "^15.9.0",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "msw": "^2.2.1",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.1",
    "ts-morph": "^21.0.1",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "typescript-eslint": "^7.0.2",
    "vite": "^5.1.4",
    "vitest": "^1.3.1"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,md}": "prettier --write"
  }
}